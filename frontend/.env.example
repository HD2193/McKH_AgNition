# Kisan AI Environment Variables
# Copy this file to .env and fill in your API keys

# Backend URL (DO NOT MODIFY)
REACT_APP_BACKEND_URL=http://localhost:3000

# Google Vision API for Crop Disease Diagnosis
REACT_APP_GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Vertex AI for Speech-to-Text and Text-to-Speech
REACT_APP_VERTEX_API_KEY=your_vertex_ai_api_key_here

# Gemini Pro API for AI Assistant
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Mandi Price Data API (Government of India)
REACT_APP_MANDI_API_KEY=your_mandi_api_key_here

# Firebase Configuration for Tasks and Calendar
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_firebase_app_id

# Additional API Keys (if needed)
REACT_APP_WEATHER_API_KEY=your_weather_api_key_here
REACT_APP_MAPS_API_KEY=your_maps_api_key_here